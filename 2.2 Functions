var topic = "Functions"
print("Hello \(topic)!")

//sample code below with 2 PARAMETERs (also works with 1 XD)

func multiplier(value1: Int, value2: Int ) {
	let result = value1 * value2
	print("The result is \(result)")
}

multiplier(value1: 30, value2: 2)

//RESULT: WORK WITH REPL.IT and iSWIFT.org not with JDOODLE
